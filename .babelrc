{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-promise-to-bluebird",
    "transform-runtime",
    ["babel-root-import", [{
      "rootPathPrefix": "~",
      "rootPathSuffix": "."
    }, {
      "rootPathPrefix": "@",
      "rootPathSuffix": "src"
    }]]
  ]
}
